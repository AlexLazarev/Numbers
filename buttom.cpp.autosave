#include "buttom.h"



Buttom::Buttom(QString name, QGraphicsItem *parent):QGraphicsRectItem(parent)
{
    setRect(0,0,200,500);
            
    QBrush brush;
    
    brush.setStyly(Qt::SolidPattern);
    brush.setColor(Qt::darkCyan);
    
    setBrush(brush);
    
    text = new QGraphicsTextItem(name,this);
    
    
}

